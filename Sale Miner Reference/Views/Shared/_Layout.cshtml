<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - MDB in ASP.NET</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@

    <!--  -->
    <!--md bootstrap additions begin-->
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <!-- Bootstrap core CSS -->
    <link href="~/Content/bootstrap.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="~/Content/mdb.min.css" rel="stylesheet">
    <!-- MDBootstrap Datatables  -->
    <link href="~/Content/MDBootstrap/css/addons/datatables.min.css" rel="stylesheet">
    <!-- Your custom styles (optional) -->
    <link href="" rel="stylesheet">

</head>
<body style="margin:0px">

    <nav class="navbar fixed-top navbar-expand-lg navbar-dark primary-color">
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <!-- Navbar brand -->
                <a class="navbar-brand" href="#"><img src="data:image/png;base64," height="30" alt="mdb logo"></a>
                <li class="nav-item">
                    <a class="nav-link" href="/SalesLanding">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="SalesCustomer?customerCode=%">All Equipment Sales</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/machinesW_kitsAvlbl_BnotInstalled">All Opportunity Sales</a>
                </li>
                <!-- Dropdown -->
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">Customers</a>
                    <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                        <a class="dropdown-item" href="/customerMgmt">Customer Management</a>
                        <a class="dropdown-item" href="/CustomerEqpmtMgmt">Customer Equipment</a>
                        <a class="dropdown-item" href="/customerEvent">Customer Events</a>
                        <a class="dropdown-item" href="/EventType">Customer Event Types</a>
                    </div>
                </li>
                <!-- Dropdown -->
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">Equipment</a>
                    <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                        <a class="dropdown-item" href="/EqpmtTypeMgmt">Equipment Type Management</a>
                        <a class="dropdown-item" href="/EqpmtMgmt">Equipment Management</a>
                        @*<a class="dropdown-item" href="/CurrentEqpmtKits">Current Eqpmt Kits</a>*@
                    </div>
                </li>
                <!-- Dropdown -->
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">Kits</a>
                    <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                        <a class="dropdown-item" href="/KitMgmt">Kit Management</a>
                        <a class="dropdown-item" href="/KitsAvlblMgmt">Kits Available</a>
                    </div>
                </li>

                <li class="nav-item " onclick="window.print()">
                    <a class="nav-link" id="navbarDropdownMenuLink" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">Print Page</a>
                </li>
            </ul>          
            @*@Html.Partial("_LoginPartial")*@
        </div>
    </nav>

    @*<div class="container body-content">*@
<div class="container-fluid">
    @RenderBody()
    <footer>
        <div  >            
        </div>
    </footer>
</div>

 <!-- jQuery -->
    <script type="text/javascript" src="~/Content/MDBootstrap/js/jquery.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="~/Content/MDBootstrap/js/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="~/Content/MDBootstrap/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="~/Content/MDBootstrap/js/mdb.min.js"></script>
    <!-- Your custom scripts (optional) -->
    <script type="text/javascript"></script>
    <!-- MDBootstrap Datatables  -->
    <script type="text/javascript" src="~/Content/MDBootstrap/js/addons/datatables.min.js"></script>   
</body>
</html>